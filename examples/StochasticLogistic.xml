<!-- Single stochastic logistic model population size trajectory  -->

<beast version='2.0' namespace='hamlet.beast:beast.core.parameter'>
    <run spec='Trajectory'
         simulationTime='20'
         verbosity='2'>
        
        <!-- <stepper spec='TauLeapingStepper' stepSize="0.01" /> -->
        <stepper spec='GillespieStepper' />

        <model spec='Model' id='model'>
            <population spec='Population' populationName="X" id='X'/>
            <reaction spec='Reaction' reactionName="Birth" rate="1.0">
                <reactant idref='X'/>
                <product idref='X'/>
                <product idref='X'/>
            </reaction>
            <reaction spec='Reaction' reactionName="Death" rate="0.01">
                <reactant idref='X'/>
                <reactant idref='X'/>
                <product idref='X'/>
            </reaction>
        </model>

        <initialState spec='InitState'>
            <populationSize spec='PopulationSize' size='1'>
                <population idref='X'/>
            </populationSize>
        </initialState>
        
        <output spec='JsonOutput' fileName='out.json'/>
    </run>
</beast>
