<!-- Generate a coalescent tree from a given number of taxa, with
     a subset of these taxa sampled at times earlier than the present. -->

<beast version='2.0' namespace='master.beast:beast.core.parameter'>
    <run spec='InheritanceTrajectory'
         verbosity='2'>
        
        <model spec='InheritanceModel'>
            <population spec='Population' populationName='X' id='X'/>
            <reaction spec='InheritanceReaction' reactionName='Coalescence' rate="1.0">
                2X:1 -> X:1
            </reaction>
        </model>                

        <initialState spec='InitState'>
            <lineageSeedMultiple spec='MultipleIndividuals' population='@X' copies="20"/>
	    <lineageSeed spec='Individual' population='@X' time="1"/>
	    <lineageSeed spec='Individual' population='@X' time="2"/>
	    <lineageSeed spec='Individual' population='@X' time="3"/>
        </initialState>
        
        <lineageEndCondition spec='LineageEndCondition' nLineages="1"/>
                
        <output spec='NexusOutput' fileName='SeriallySampledCoalescentTree_output.nexus' reverseTime="true"/>
    </run>
</beast>
